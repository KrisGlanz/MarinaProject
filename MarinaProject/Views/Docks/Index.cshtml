@model IEnumerable<MarinaProject.Models.Dock>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<ul class="nav nav-tabs">
    <li class="nav-item"><a class="nav-link active" href="../Docks/Index">Docks</a></li>
    <li class="nav-item"><a class="nav-link active" href="../Slips/Index">Slips</a></li>
</ul>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Electricity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Water)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Electricity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Water)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DockId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DockId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DockId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
